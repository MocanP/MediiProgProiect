    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Cumparare</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cumparare.Membru)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cumparare.Membru.ID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cumparare.MasinaID)
        </dt>
        <dd class="col-sm-10">
